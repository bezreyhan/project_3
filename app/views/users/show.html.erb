<%= render partial: '/layouts/navbar' %>
<h1 id="title"><u>My Profile</u></h1>
<h3 id="myProfile1">
    <%= current_user ? current_user.username : "(logged in)" %> 
</h3>

  
  <div>
    <u id="myProfile">Name:</u> <br />
    <%= current_user.full_name %><br />
    <% if current_user.avatar_file_size %>
        <%= image_tag current_user.avatar.url(:medium) if current_user.avatar.file? %>
    <% end %>
    <br/>
    <u id="myProfile">Email:</u> <br />
    <%= current_user.email %><br />
    <u id="myProfile">Location:</u><br /> 
    <%= current_user.location %><br />
    <u id="myProfile">Interests:</u><br />
    <% unless current_user.interests.empty? %>
        <%= current_user.interests.first.name %>
    <% end %>
  </div>
<br />
<%= link_to "Home", users_path %><br />
